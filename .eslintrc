{
  "extends": [
    "@favware/eslint-config"
  ],
  "parserOptions": {
    "createDefaultProgram": true,
  },
  "rules": {
    "new-cap": [
      "error",
      {
        "capIsNewExceptionPattern": "^[A-Z][a-z]+(?:[A-Z][a-z]+)*$"
      }
    ],
    "multiline-comment-style": "off",
    "capitalized-comments": "off",
    "line-comment-position":"off",
    "@typescript-eslint/no-unused-vars": "off"
  }
}